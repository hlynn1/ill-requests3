<header class="navbar navbar-fixed-top">
	<div class="navbar-inner">
		<div class="container">
			<% if signed_in? %>
				<%= link_to "ill requests", requests_path, id: "logo" %>
			<% else %>
				<%= link_to "ill requests", root_path, id: "logo" %>
			<% end %>
			<nav>
				<ul class="nav pull-right">
					<% if signed_in? %>
						<li><%= link_to "Requests", requests_path %></li>
						<li><%= link_to "Customers", customers_path %></li>
						<li><%= link_to "Sign Out", signout_path, method: "delete" %></li>
					<% else %>
						<li><%= link_to "Requests", all_requests_path %>
						<li><%= link_to "Sign In", signin_path %></li>
					<% end %>
				</ul>
			</nav>
		</div>
	</div>
</header>
